.ragister-account.text-center.tr-section.tr-section-padding
  .account-content
    .section-title
      %h1= t :forgotpass

    = bootstrap_form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post, class: 'contact-form' }) do |f|
      = f.email_field :email, autofocus: true, autocomplete: 'email', placeholder: 'email', hide_label: true
      = f.primary t(:resend)

  .options= render '/users/links'