= search_form_for @q, html: { class: 'form-inline mb-4' } do |f|
  .input-group.col-12.px-0
    = f.search_field :casetitle_cont_any, class: 'form-control', placeholder: t('organizations.search')
    .input-group-append
      = f.button(type: 'submit', class: 'btn btn-primary' ) do
        = fa_icon 'search'
